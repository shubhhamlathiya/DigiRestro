<!doctype html>
<html lang="en" dir="ltr">
<head>
    <title>Branch - Food Order</title>
    <!-- Library / Plugin Css Build -->
    <link rel="stylesheet" href="assets/css/core/libs.min.css">
    <!-- Custom Css -->
    <link rel="stylesheet" href="assets/css/aprycot.mine209.css?v=1.0.0">
    <style>
        .box {
            width: 30%;
        }
    </style>
</head>
<body>

<%- include("sidebar.ejs") %>

<main class="main-content">
    <div class="position-sticky">
        <%- include("nav.ejs") %>
    </div>
    <div class="content-inner mt-5 py-0">

        <div class="row">
            <div class="col-lg-2">
                <div class="card">
                    <div class="card-header border-0">
                        <select class="card-title heading-title">
                            <option class="heading-title">Fast Food</option>
                            <option class="heading-title">Bar</option>
                            <option class="heading-title">Drinks</option>
                            <option class="heading-title">Food Court</option>
                        </select>
                    </div>
                    <div class="card-body py-0">
                        <ul class="list-inline">
                            <li class="py-1 d-flex justify-content-between">
                                <span class="fw-bolder heading-title text-dark">Starters</span>
                            </li>
                            <li class="py-1 d-flex justify-content-between">
                                <span class="fw-bolder heading-title text-dark">Main Course</span>
                            </li>
                            <li class="py-1 d-flex justify-content-between">
                                <span class="fw-bolder heading-title text-dark">Soups</span>
                            </li>
                            <li class="py-1 d-flex justify-content-between">
                                <span class="fw-bolder heading-title text-dark">Juices</span>
                            </li>
                            <li class="py-1 d-flex justify-content-between">
                                <span class="fw-bolder heading-title text-dark">Italian</span>
                            </li>
                            <li class="py-1 d-flex justify-content-between">
                                <span class="fw-bolder heading-title text-dark">Chinese</span>
                            </li>
                            <li class="py-1 d-flex justify-content-between">
                                <span class="fw-bolder heading-title text-dark">Pizza</span>
                            </li>
                            <li class="py-1 d-flex justify-content-between pb-0">
                                <span class="fw-bolder heading-title text-dark">Deserts</span>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>

            <div class="col-lg-7">
                <div class="card">
                    <div class="card-header ">
                        <h4 class="card-title list-main">Starters</h4>
                    </div>
                    <div class="card-body pt-1 mt-1">
                        <div class="row">
                            <div class="box ">
                                <div class="text-center bg-success">
                                    <h6>Top Left</h6>
                                </div>
                            </div>
                            <div class="col-sm-6 col-md-6 col-lg-3 mt-3 mt-md-0">
                                <div class="top-left p-4 shadow-showcase text-center bg-success">
                                    <h6>Top Left</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-lg-3">

        </div>
    </div>

</main>
<%- include("links.ejs") %>
</body>
</html>